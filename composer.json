{
    "name": "kylegg/web-utilities",
    "description": "Helper classes for Laravel",
    "keywords": ["laravel", "webutilities", "helper"],
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Kylegg\\WebUtilities\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Kylegg\\WebUtilities\\Tests\\": "tests/"
        }
    },
    "extra" : {
        "laravel": {
            "dont-discover": [
                "kylegg/web-utilities"
            ]
		}
    },
    "require-dev": {
        "phpunit/phpunit": "^10.3",
        "orchestra/testbench": "^8.10"
    },
    "scripts": {
        "test": "phpunit"
    }
}
